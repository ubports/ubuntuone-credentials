<?xml version="1.0" encoding="UTF-8"?>
<provider id="ubuntuone">
  <name>Ubuntu One</name>
  <icon>ubuntu</icon>
  <translations>account-plugins</translations>
  <domains>.*ubuntu\.com</domains>
  <plugin>ubuntuone</plugin>

  <template>
    <group name="auth">
      <setting name="method">ubuntuone</setting>
      <setting name="mechanism">ubuntuone</setting>
      <group name="ubuntuone">
	<setting name="TEST">WHEE</setting>
      </group>
    </group>
  </template>
</provider>
